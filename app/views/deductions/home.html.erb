<div class="page-header">
	<h1>Latest Deductions</h1>
</div>
<ul class="nav nav-tabs nav-stacked">
<% @deductions.each do |deductions| %>
  <li>
    <%= link_to(deductions) do %>
      <%= deductions.name %> :- <%= deductions.amount %>
    <% end %>
  </li>
<% end %>
</ul>
<% if @deductions.count == 5 %>
<ul class="nav nav-tabs nav-stacked">
  <li><%= link_to 'All Deductions', deductions_path %></li>
</ul>
<% end %>